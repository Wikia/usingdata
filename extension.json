{
	"name": "UsingData",
	"version": "2.1.0",
	"author": "foxlit",
	"descriptionmsg": "usingdata-description",
	"license-name": "GPL-2.0-or-later",
	"type": "parserhook",
	"requires": {
		"MediaWiki": ">= 1.39.0"
	},
	"MessagesDirs": {
		"UsingData": [
			"i18n"
		]
	},
	"ExtensionMessagesFiles": {
		"UsingDataMagic": "UsingData.i18n.magic.php"
	},
